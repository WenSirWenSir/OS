<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/color_success"    android:orientation="vertical">    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginTop="25dp">        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_centerHorizontal="true"            android:text="登录"            android:textColor="#ffffff"            android:textSize="18sp"            android:textStyle="bold" />    </RelativeLayout>    <!--显示一个货的字体-->    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginTop="100dp">        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="Free"            android:textColor="#020433"            android:textSize="35sp"            android:textStyle="bold" />    </LinearLayout></RelativeLayout>