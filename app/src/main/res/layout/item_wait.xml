<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="80dp"    android:layout_height="80dp"    android:background="#efefef">    <ImageView        android:id="@+id/item_wait_img"        android:layout_width="40dp"        android:layout_height="40dp"        android:layout_centerInParent="true"        android:src="@drawable/ico_wait" />    <TextView        android:id="@+id/item_wait_title"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_alignParentBottom="true"        android:layout_centerHorizontal="true"        android:gravity="center"        android:textColor="#000000"        android:textSize="10sp" /></RelativeLayout>