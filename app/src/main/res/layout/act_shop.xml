<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="#ffffff"    android:fitsSystemWindows="false">    <com.bigkoo.convenientbanner.ConvenientBanner        android:id="@+id/act_shopBanner"        android:layout_width="match_parent"        android:layout_height="match_parent">    </com.bigkoo.convenientbanner.ConvenientBanner>    <!--添加倒计时-->    <RelativeLayout        android:id="@+id/act_shopdownTimeBody"        android:layout_width="200dp"        android:layout_height="40dp"        android:layout_centerHorizontal="true"        android:layout_marginTop="45dp"        android:background="#efefef"        android:elevation="5dp"        android:orientation="horizontal"        android:padding="2dp"        android:translationZ="5dp">        <ImageView            android:layout_width="30dp"            android:layout_height="30dp"            android:layout_centerVertical="true"            android:layout_marginLeft="10dp"            android:src="@drawable/ico_downtime" />        <LinearLayout            android:layout_width="200dp"            android:layout_height="match_parent"            android:orientation="vertical">            <TextView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:gravity="center"                android:text="倒计时"                android:textColor="#020433"                android:textSize="10sp" />            <os.zbwlkj.com.os.Views.SystemPriceTextView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:gravity="center"                android:text="20:89"                android:textColor="#020433"                android:textSize="13sp"                android:textStyle="bold" />        </LinearLayout>    </RelativeLayout>    <RelativeLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_alignParentBottom="true"        android:layout_marginBottom="10dp">        <RelativeLayout            android:id="@+id/act_shopBtnBody"            android:layout_width="250dp"            android:layout_height="50dp"            android:layout_centerInParent="true"            android:background="#efefef">            <LinearLayout                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_marginLeft="35dp"                android:orientation="vertical">                <ImageView                    android:layout_width="30dp"                    android:layout_height="30dp"                    android:src="@drawable/ico_btnmain" />                <TextView                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:gravity="center"                    android:text="首页"                    android:textColor="#000000"                    android:textSize="10sp" />            </LinearLayout>            <LinearLayout                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_alignParentRight="true"                android:layout_centerVertical="true"                android:layout_marginRight="35dp"                android:orientation="vertical">                <ImageView                    android:layout_width="30dp"                    android:layout_height="30dp"                    android:src="@drawable/ico_btnuser" />                <TextView                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:gravity="center"                    android:text="我的"                    android:textColor="#000000"                    android:textSize="10sp" />            </LinearLayout>        </RelativeLayout>        <!--圆形按钮控件-->        <RelativeLayout            android:id="@+id/act_shopBtnPayBody"            android:layout_width="75dp"            android:layout_height="75dp"            android:layout_centerInParent="true"            android:layout_margin="10dp">            <RelativeLayout                android:id="@+id/act_shopBtnPay"                android:layout_width="65dp"                android:layout_height="65dp"                android:layout_centerInParent="true"                android:background="@drawable/border_buttonripple">                <ImageView                    android:layout_width="30dp"                    android:layout_height="30dp"                    android:layout_centerHorizontal="true"                    android:layout_marginTop="5dp"                    android:src="@drawable/ico_basket" />                <os.zbwlkj.com.os.Views.SystemTextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentBottom="true"                    android:layout_centerInParent="true"                    android:layout_marginBottom="10dp"                    android:text="Pay"                    android:textColor="#ffffff"                    android:textSize="15sp" />            </RelativeLayout>        </RelativeLayout>    </RelativeLayout>    <!--设置右边的按钮-->    <LinearLayout        android:id="@+id/act_shopRightBtnbody"        android:layout_width="50dp"        android:layout_height="50dp"        android:layout_alignParentRight="true"        android:layout_centerVertical="true"        android:layout_marginRight="10dp"        android:background="#efefef"        android:elevation="5dp"        android:orientation="vertical"        android:translationZ="5dp">        <RelativeLayout            android:layout_width="50dp"            android:layout_height="50dp">            <ImageView                android:id="@+id/act_shopBtnhideNav"                android:layout_width="30dp"                android:layout_height="30dp"                android:layout_centerHorizontal="true"                android:src="@drawable/ico_hide" />            <TextView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_below="@id/act_shopBtnhideNav"                android:layout_marginTop="2dp"                android:gravity="center"                android:text="隐藏"                android:textColor="#000000"                android:textSize="10sp" />        </RelativeLayout>    </LinearLayout>    <!--设置左边的按钮-->    <LinearLayout        android:id="@+id/act_shopleftBtnbody"        android:layout_width="50dp"        android:layout_height="wrap_content"        android:layout_centerVertical="true"        android:layout_marginLeft="10dp"        android:background="#efefef"        android:elevation="5dp"        android:orientation="vertical"        android:translationZ="5dp">        <RelativeLayout            android:layout_width="50dp"            android:layout_height="60dp"            android:layout_marginTop="10dp">            <ImageView                android:id="@+id/act_shopBtnbrightSpot"                android:layout_width="30dp"                android:layout_height="30dp"                android:layout_centerHorizontal="true"                android:src="@drawable/ico_brightspot" />            <TextView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_below="@id/act_shopBtnbrightSpot"                android:layout_marginTop="2dp"                android:gravity="center"                android:text="亮点"                android:textColor="#000000"                android:textSize="10sp" />        </RelativeLayout>        <RelativeLayout            android:layout_width="50dp"            android:layout_height="60dp"            android:layout_marginTop="10dp">            <ImageView                android:id="@+id/act_shopBtnbusiness"                android:layout_width="30dp"                android:layout_height="30dp"                android:layout_centerHorizontal="true"                android:src="@drawable/ico_business" />            <TextView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_below="@id/act_shopBtnbusiness"                android:layout_marginTop="2dp"                android:gravity="center"                android:text="商家"                android:textColor="#000000"                android:textSize="10sp" />        </RelativeLayout>        <RelativeLayout            android:layout_width="50dp"            android:layout_height="60dp"            android:layout_marginTop="10dp">            <ImageView                android:id="@+id/act_shopBtnshopMsg"                android:layout_width="30dp"                android:layout_height="30dp"                android:layout_centerHorizontal="true"                android:src="@drawable/ico_shopmsg" />            <TextView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_below="@id/act_shopBtnshopMsg"                android:layout_marginTop="2dp"                android:gravity="center"                android:text="详情"                android:textColor="#000000"                android:textSize="10sp" />        </RelativeLayout>        <RelativeLayout            android:layout_width="50dp"            android:layout_height="60dp"            android:layout_marginTop="10dp">            <ImageView                android:id="@+id/act_shopBtnsupport"                android:layout_width="33dp"                android:layout_height="33dp"                android:layout_centerHorizontal="true"                android:src="@drawable/ico_support" />            <TextView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_below="@id/act_shopBtnsupport"                android:layout_marginTop="2dp"                android:gravity="center"                android:text="1000赞"                android:textColor="#000000"                android:textSize="10sp" />        </RelativeLayout>    </LinearLayout></RelativeLayout>