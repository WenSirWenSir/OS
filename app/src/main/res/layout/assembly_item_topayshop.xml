<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="#efefef"    android:orientation="vertical">    <ScrollView        android:layout_width="match_parent"        android:layout_height="match_parent">        <!--设置收件地址缩写-->        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:orientation="vertical">            <LinearLayout                android:id="@+id/act_topayshopAddrbody"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_margin="20dp"                android:orientation="horizontal">                <RelativeLayout                    android:layout_width="70dp"                    android:layout_height="70dp">                    <ImageView                        android:layout_width="40dp"                        android:layout_height="40dp"                        android:layout_centerInParent="true"                        android:src="@drawable/ico_addr" />                </RelativeLayout>                <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="match_parent">                    <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_centerVertical="true"                        android:orientation="vertical">                        <LinearLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content">                            <TextView                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:text="翁启鑫"                                android:textColor="@color/SystemTextColor"                                android:textSize="13sp" />                            <os.zbwlkj.com.os.Views.SystemNumerView                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_marginLeft="5dp"                                android:text="15206036936"                                android:textColor="#666666"                                android:textSize="10sp" />                        </LinearLayout>                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_marginTop="5dp"                            android:text="福建省龙岩市上杭县旧县乡梅溪村岗上路21号"                            android:textColor="#000000"                            android:textSize="10sp" />                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_marginTop="5dp"                            android:text="无法收货的时候,可免费寄存至快递点"                            android:textColor="@color/SystemTheme"                            android:textSize="10sp" />                    </LinearLayout>                </RelativeLayout>            </LinearLayout>            <!--设置购买的信息-->            <LinearLayout                android:id="@+id/act_topayshopMsgBody"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginLeft="10dp"                android:layout_marginRight="10dp"                android:background="#ffffff"                android:orientation="vertical"                android:padding="5dp">                <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="20dp">                    <ImageView                        android:layout_width="20dp"                        android:layout_height="20dp"                        android:src="@drawable/ico_program" />                    <TextView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_centerVertical="true"                        android:layout_marginLeft="25dp"                        android:text="龙岩市左边网络科技有限公司"                        android:textColor="@color/SystemTextColor"                        android:textSize="10sp" />                </RelativeLayout>                <!--显示商品的图片 信息  价格 数量-->                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="10dp"                    android:orientation="horizontal">                    <LinearLayout                        android:layout_width="80dp"                        android:layout_height="80dp"                        android:layout_weight="0">                    </LinearLayout>                    <!--显示商品的标题 套餐 发货时间 -->                    <LinearLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="2"                        android:orientation="vertical">                        <TextView                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:maxLines="2"                            android:paddingLeft="2dp"                            android:paddingRight="2dp"                            android:text="海天750危机向上岛咖啡焦磷酸看得见风立减"                            android:textColor="#000000"                            android:textSize="10sp" />                        <TextView                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:layout_marginLeft="3dp"                            android:layout_marginTop="3dp"                            android:layout_marginRight="3dp"                            android:background="#efefef"                            android:maxLines="2"                            android:paddingLeft="2dp"                            android:paddingTop="2dp"                            android:paddingRight="2dp"                            android:paddingBottom="2dp"                            android:text="3排36瓶装;"                            android:textColor="#000000"                            android:textSize="8sp" />                    </LinearLayout>                    <LinearLayout                        android:layout_width="0dp"                        android:layout_height="80dp"                        android:layout_weight="1"                        android:orientation="vertical">                        <!--显示价格-->                        <RelativeLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content">                            <LinearLayout                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_alignParentRight="true"                                android:layout_marginRight="8dp">                                <TextView                                    android:layout_width="wrap_content"                                    android:layout_height="wrap_content"                                    android:text="￥:"                                    android:textColor="#666666"                                    android:textSize="8sp" />                                <os.zbwlkj.com.os.Views.SystemNumerView                                    android:layout_width="wrap_content"                                    android:layout_height="wrap_content"                                    android:text="34.00"                                    android:textColor="@color/SystemTheme"                                    android:textSize="10sp"                                    android:textStyle="bold" />                            </LinearLayout>                        </RelativeLayout>                        <!--显示数量-->                        <RelativeLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:layout_marginTop="5dp">                            <LinearLayout                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_alignParentRight="true"                                android:layout_marginRight="8dp">                                <os.zbwlkj.com.os.Views.SystemNumerView                                    android:layout_width="wrap_content"                                    android:layout_height="wrap_content"                                    android:text="x1"                                    android:textColor="#666666"                                    android:textSize="10sp" />                            </LinearLayout>                        </RelativeLayout>                    </LinearLayout>                </LinearLayout>                <!--购买数量-->                <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginLeft="20dp"                    android:layout_marginTop="5dp"                    android:layout_marginRight="20dp"                    android:padding="5dp">                    <TextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_alignParentLeft="true"                        android:layout_centerVertical="true"                        android:text="购买数量"                        android:textColor="#000000"                        android:textSize="10sp" />                    <LinearLayout                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_alignParentRight="true"                        android:layout_marginRight="15dp"                        android:padding="1dp">                        <LinearLayout                            android:layout_width="wrap_content"                            android:layout_height="wrap_content">                            <TextView                                android:id="@+id/assembly_item_topayshopBtnDel"                                android:layout_width="18dp"                                android:layout_height="18dp"                                android:background="@drawable/border_btn_del"                                android:gravity="center"                                android:text="-"                                android:textColor="#a9a9a9" />                        </LinearLayout>                        <LinearLayout                            android:layout_width="wrap_content"                            android:layout_height="wrap_content">                            <os.zbwlkj.com.os.Views.SystemNumerView                                android:id="@+id/assembly_item_topayshopNumber"                                android:layout_width="30dp"                                android:layout_height="18dp"                                android:background="@drawable/border_inputnumber"                                android:gravity="center"                                android:text="1"                                android:textColor="#a9a9a9"                                android:textSize="10sp" />                        </LinearLayout>                        <LinearLayout                            android:layout_width="wrap_content"                            android:layout_height="wrap_content">                            <TextView                                android:id="@+id/assembly_item_topayshopBtnAdd"                                android:layout_width="18dp"                                android:layout_height="18dp"                                android:background="@drawable/border_btn_add"                                android:gravity="center"                                android:text="+"                                android:textColor="#a9a9a9"                                android:textSize="10sp"                                />                        </LinearLayout>                    </LinearLayout>                </RelativeLayout>                <!--配送方式-->                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginLeft="20dp"                    android:layout_marginTop="5dp"                    android:layout_marginRight="20dp"                    android:padding="5dp">                    <TextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_weight="0"                        android:text="配送方式"                        android:textColor="#000000"                        android:textSize="10sp"                        />                    <TextView                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:paddingLeft="10dp"                        android:text="普通配送"                        android:textColor="#666666"                        android:textSize="10sp" />                    <LinearLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1">                    </LinearLayout>                </LinearLayout>                <!--破损险-->                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginLeft="20dp"                    android:layout_marginTop="5dp"                    android:layout_marginRight="20dp"                    android:padding="5dp">                    <TextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="破送损险"                        android:textColor="#000000"                        android:textSize="10sp" />                    <TextView                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:paddingLeft="10dp"                        android:text="商家赠送"                        android:textColor="#666666"                        android:textSize="10sp" />                    <LinearLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1">                    </LinearLayout>                </LinearLayout>                <!--运送险-->                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginLeft="20dp"                    android:layout_marginTop="5dp"                    android:layout_marginRight="20dp"                    android:baselineAligned="false"                    android:orientation="horizontal"                    android:padding="5dp">                    <RelativeLayout                        android:layout_width="wrap_content"                        android:layout_height="20dp"                        android:layout_weight="0"                        android:background="#ffffff">                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_centerVertical="true"                            android:text="运送保险"                            android:textColor="#000000"                            android:textSize="10sp" />                    </RelativeLayout>                    <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="20dp"                        android:layout_marginLeft="10dp"                        android:layout_weight="1">                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_centerVertical="true"                            android:text="最高获赔9.00运费"                            android:textColor="#666666"                            android:textSize="10sp" />                    </RelativeLayout>                    <RelativeLayout                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:background="#ffffff">                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_centerVertical="true"                            android:layout_marginRight="5dp"                            android:layout_toLeftOf="@id/assembly_item_topayshopBtnSelect"                            android:text="￥:1.0"                            android:textColor="@color/SystemTextColor"                            android:textSize="10sp" />                        <ImageView                            android:id="@+id/assembly_item_topayshopBtnSelect"                            android:layout_width="20dp"                            android:layout_height="20dp"                            android:layout_alignParentRight="true"                            android:layout_marginRight="15dp"                            android:src="@drawable/ico_select" />                    </RelativeLayout>                </LinearLayout>                <!--备注-->                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginLeft="20dp"                    android:layout_marginTop="5dp"                    android:layout_marginRight="20dp"                    android:padding="5dp">                    <TextView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="商品备注"                        android:textColor="#000000"                        android:textSize="10sp" />                    <EditText                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_marginLeft="10dp"                        android:background="@null"                        android:focusable="false"                        android:focusableInTouchMode="false"                        android:text="选填,填写比如收件不方便的联系电话"                        android:textColor="#666666"                        android:textSize="10sp" />                </LinearLayout>                <!--商品的小计金额-->                <RelativeLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginLeft="20dp"                    android:layout_marginTop="5dp"                    android:layout_marginRight="20dp"                    android:padding="5dp">                    <LinearLayout                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_alignParentRight="true">                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_gravity="bottom"                            android:text="共1件,"                            android:textColor="#a9a9a9"                            android:textSize="8sp" />                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_alignParentRight="true"                            android:layout_gravity="bottom"                            android:text="小计:"                            android:textColor="#666666"                            android:textSize="8sp" />                        <LinearLayout                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_alignParentRight="true"                            android:layout_marginRight="8dp">                            <TextView                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:text="￥:"                                android:textColor="#666666"                                android:textSize="5sp" />                            <os.zbwlkj.com.os.Views.SystemNumerView                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:text="34.00"                                android:textColor="@color/SystemTheme"                                android:textSize="8sp" />                        </LinearLayout>                    </LinearLayout>                </RelativeLayout>            </LinearLayout>            <!--设置确定信息下一步按钮-->            <RelativeLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginTop="20dp"                android:background="#efefef">                <TextView                    android:id="@+id/assembly_item_topayshopBtnNext"                    android:layout_width="150dp"                    android:layout_height="30dp"                    android:layout_centerInParent="true"                    android:gravity="center"                    android:text="下一步"                    android:textColor="#020433"                    android:textSize="10sp" />            </RelativeLayout>        </LinearLayout>    </ScrollView></LinearLayout>