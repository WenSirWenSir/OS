<?xml version="1.0" encoding="utf-8"?><shape xmlns:android="http://schemas.android.com/apk/res/android">    <!--设置背景填充色-->    <solid android:color="#ffffff" />    <!--设置边框为1dp 边框的颜色-->    <stroke        android:width="1dp"        android:color="#a9a9a9" />    <!--设置按钮的圆半径为10dp-->    <corners        android:bottomRightRadius="2dp"        android:topRightRadius="2dp" /></shape>